<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <size android:height="32dp" android:width="32dp"></size>
            <gradient
                android:startColor="@color/darkgreen"
                android:endColor="@color/green"
                android:angle="90" />
            <stroke
                android:width="1dp"
                android:color="@color/darkred" />
        </shape>
    </item>
    <item android:bottom="6dp" android:top="6dp" android:left="8dp" android:right="18dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/darkblue" />
        </shape>
    </item>
    <item android:bottom="6dp" android:top="6dp" android:left="18dp" android:right="8dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/darkblue" />
        </shape>
    </item>
</layer-list>